  "devDependencies": {
    "@types/chai": "^4",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.21",
    "@types/mocha": "^10",
    "@types/node": "^22.5.4",
    "chai": "^5.1.1",
    "mocha": "^10.1.0",
    "mockserver": "^2.0.0",
    "nyc": "^17.1.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
